<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:background="@color/background_material_dark"

    android:layout_width="match_parent"
    android:layout_height="match_parent">


    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/button"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:background="@drawable/p2_tray"
        android:text="TRAY" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/button2"
        android:layout_alignParentBottom="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:background="@drawable/p2_tray" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/button3"
        android:textColor="@color/abc_search_url_text"
        android:layout_alignParentBottom="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:background="@drawable/p1_tray"
        android:text="TRAY" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/button4"
        android:layout_alignParentTop="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:background="@drawable/p1_tray" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="0"
        android:textColor="@color/abc_primary_text_material_light"
        android:id="@+id/button9"
        android:layout_below="@+id/button"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_alignRight="@+id/button2"
        android:layout_alignEnd="@+id/button2"
        android:background="@drawable/p2_tray"
        android:layout_above="@+id/button2" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="0"
        android:textColor="@color/abc_search_url_text"
        android:id="@+id/button10"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:layout_alignTop="@+id/button9"
        android:layout_alignLeft="@+id/button3"
        android:layout_alignStart="@+id/button3"
        android:background="@drawable/p1_tray"
        android:layout_above="@+id/button3" />

    <LinearLayout
        android:orientation="horizontal"
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:gravity="center"
        android:layout_alignBottom="@+id/button"
        android:layout_toRightOf="@+id/button"
        android:layout_toLeftOf="@+id/button4"
        android:layout_toStartOf="@+id/button4"
        android:id="@+id/linearLayout"
        android:weightSum="1">

        <Button
            style="?android:attr/buttonStyleSmall"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="3"
            android:textColor="@color/abc_primary_text_material_light"
            android:id="@+id/button5"
            android:background="@drawable/bowl_p2_bg_selector"
            android:layout_above="@+id/button9"
            android:layout_toRightOf="@+id/button" />

        <Space
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_weight="0.05" />

        <Button
            style="?android:attr/buttonStyleSmall"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="3"
            android:textColor="@color/abc_primary_text_material_light"
            android:id="@+id/button6"
            android:background="@drawable/bowl_p2_bg_selector"
            android:layout_above="@+id/button9"
            android:layout_toRightOf="@+id/button" />

        <Space
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_weight="0.05" />

        <Button
            style="?android:attr/buttonStyleSmall"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="3"
            android:textColor="@color/abc_primary_text_material_light"
            android:id="@+id/button7"
            android:background="@drawable/bowl_p2_bg_selector"
            android:layout_above="@+id/button9"
            android:layout_toRightOf="@+id/button" />

        <Space
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_weight="0.05" />

        <Button
            style="?android:attr/buttonStyleSmall"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="3"
            android:textColor="@color/abc_primary_text_material_light"
            android:id="@+id/button8"
            android:background="@drawable/bowl_p2_bg_selector"
            android:layout_above="@+id/button9"
            android:layout_toRightOf="@+id/button" />

        <Space
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_weight="0.05" />

        <Button
            style="?android:attr/buttonStyleSmall"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="3"
            android:textColor="@color/abc_primary_text_material_light"
            android:id="@+id/button11"
            android:background="@drawable/bowl_p2_bg_selector"
            android:layout_above="@+id/button9"
            android:layout_toRightOf="@+id/button" />

        <Space
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_weight="0.05" />

        <Button
            style="?android:attr/buttonStyleSmall"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="3"
            android:textColor="@color/abc_primary_text_material_light"
            android:id="@+id/button12"
            android:background="@drawable/bowl_p2_bg_selector"
            android:layout_above="@+id/button9"
            android:layout_toRightOf="@+id/button" />

        <Space
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_weight="0.05" />

    </LinearLayout>

    <LinearLayout
        android:orientation="horizontal"
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:gravity="center"
        android:layout_alignParentBottom="true"
        android:layout_toRightOf="@+id/button2"
        android:layout_below="@+id/button9"
        android:layout_alignRight="@+id/linearLayout"
        android:layout_alignEnd="@+id/linearLayout"
        android:weightSum="1"
        android:id="@+id/linearLayout2">

        <Button
            style="?android:attr/buttonStyleSmall"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="3"
            android:textColor="@color/abc_search_url_text"
            android:id="@+id/button13"
            android:background="@drawable/bowl_p1_bg_selector"
            android:layout_above="@+id/button9"
            android:layout_toRightOf="@+id/button" />

        <Space
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_weight="0.05" />

        <Button
            style="?android:attr/buttonStyleSmall"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="3"
            android:textColor="@color/abc_search_url_text"
            android:id="@+id/button14"
            android:background="@drawable/bowl_p1_bg_selector"
            android:layout_above="@+id/button9"
            android:layout_toRightOf="@+id/button" />

        <Space
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_weight="0.05" />

        <Button
            style="?android:attr/buttonStyleSmall"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="3"
            android:textColor="@color/abc_search_url_text"
            android:id="@+id/button15"
            android:background="@drawable/bowl_p1_bg_selector"
            android:layout_above="@+id/button9"
            android:layout_toRightOf="@+id/button" />

        <Space
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_weight="0.05" />

        <Button
            style="?android:attr/buttonStyleSmall"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="3"
            android:textColor="@color/abc_search_url_text"
            android:id="@+id/button16"
            android:background="@drawable/bowl_p1_bg_selector"
            android:layout_above="@+id/button9"
            android:layout_toRightOf="@+id/button" />

        <Space
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_weight="0.05" />

        <Button
            style="?android:attr/buttonStyleSmall"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="3"
            android:textColor="@color/abc_search_url_text"
            android:id="@+id/button17"
            android:background="@drawable/bowl_p1_bg_selector"
            android:layout_above="@+id/button9"
            android:layout_toRightOf="@+id/button" />

        <Space
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_weight="0.05" />

        <Button
            style="?android:attr/buttonStyleSmall"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="3"
            android:textColor="@color/abc_search_url_text"
            android:id="@+id/button18"
            android:background="@drawable/bowl_p1_bg_selector"
            android:layout_above="@+id/button9"
            android:layout_toRightOf="@+id/button" />

    </LinearLayout>

    <LinearLayout
        android:orientation="vertical"
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:layout_below="@+id/linearLayout"
        android:gravity="center"
        android:layout_toRightOf="@+id/button"
        android:layout_above="@+id/button2"
        android:layout_toLeftOf="@+id/button4"
        android:layout_toStartOf="@+id/button4">

        <EditText
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textColor="@color/abc_search_url_text"
            android:id="@+id/editText1"
            android:onClick="setName1"
            android:text="Set name Player1" />

        <EditText
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/editText2"
            android:onClick="setName2"
            android:text="Set name Player2" />

    </LinearLayout>

</RelativeLayout>